try {

  stage('Build this module') {
    dir('mod-resource-sharing') {
      sh './gradlew -Dgrails.env=okapi --no-daemon --console plain clean bootRepackage'
    }
  }
  
} finally {
  stage('Archive artifacts') {
    archiveArtifacts artifacts: 'mod-resource-sharing/build/libs/*.war', fingerprint: true
  }
}